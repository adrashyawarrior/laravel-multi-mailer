{
    "name": "adrashyawarrior/laravel-multi-mailer",
    "description": "This package allows laravel project to integrate multiple mail providers on priority bases.",
    "keywords": [
        "adrashyawarrior",
        "laravel",
        "laravel-multi-mailer"
    ],
    "homepage": "https://github.com/adrashyawarrior/laravel-multi-mailer",
    "license": "MIT",
    "authors": [
        {
            "name": "Lalit Kumar",
            "email": "lalitkumar9557758955@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "mailgun/mailgun-php": "^3.5",
        "sendinblue/api-v3-sdk": "^8.4",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Adrashyawarrior\\LaravelMultiMailer\\": "src",
            "Adrashyawarrior\\LaravelMultiMailer\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adrashyawarrior\\LaravelMultiMailer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adrashyawarrior\\LaravelMultiMailer\\LaravelMultiMailerServiceProvider"
            ],
            "aliases": {
                "LaravelMultiMailer": "Adrashyawarrior\\LaravelMultiMailer\\Facades\\LaravelMultiMailer"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
